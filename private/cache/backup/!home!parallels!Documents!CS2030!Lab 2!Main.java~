import java.util.Scanner;
import java.util.ArrayList;
import java.util.Arrays;

public class Main {
    public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);

	int numOfCruises = sc.nextInt();
	sc.nextLine();

	ArrayList<Loader> loaders = new ArrayList<>();

	for (int i = 0; i < numOfCruises; i++) {

	    String[] entry = sc.nextLine().split(" ");

	    Cruise cruise = null;

	    if (entry.length == 2) {
		cruise = new Cruise(entry[0], Integer.valueOf(entry[1]));
	    } else {
		cruise = new BigCruise(entry[0], Integer.valueOf(entry[1]), Integer.valueOf(entry[2]), Integer.valueOf(entry[3]));
	    }

	    int numOfLoaders = loaders.size();

	    for (int j = 0; j <= numOfLoaders; j++) {
		if (j == numOfLoaders) {
		    loaders.add(new Loader(numOfLoaders).serve(cruise));
		    System.out.println(loaders.get(numOfLoaders).toString());
		}
		Loader currLoader = loaders.get(j);
		if (currLoader.serve(cruise) != null) {
		    loaders.set(j, currLoader.serve(cruise));
		    System.out.println(loaders.get(j).toString());
		    break;
		}
	    }

	}
    }
}
